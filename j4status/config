# vim:ft=cfg

[Plugins]
Input=i3focus;nm;upower;time;

[Time]
format=%m %d, %H:%M

[NetworkManager]
Interfaces=wlp1s0

[NetworkManager Formats]
Addresses=
UpEthernet=${(<speed>b/s)}
UpWiFi=${ssid> }(${strength>%, }${bitrate}b/s)

[Override nm-wifi:wlp1s0]
Label=ğŸ“¶

[Override time:local]
Label=ğŸ•—

[Override upower-battery:BAT0]
Label=ğŸ”‹
