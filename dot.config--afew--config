# vim:ft=cfg

[SpamFilter]
[ClassifyingFilter]
[KillThreadsFilter]
[ListMailsFilter]

[Filter.1]
query = tag:killed and tag:unread
tags = -unread

[SentMailsFilter]
[ArchiveSentMailsFilter]
sent_tag = sent

[InboxFilter]
tags = +unread;+inbox

[MailMover]
folders = INBOX Spam
INBOX = 'tag:spam':Spam
Spam = 'not tag:spam':INBOX
